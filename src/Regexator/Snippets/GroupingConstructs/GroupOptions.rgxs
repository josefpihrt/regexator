<?xml version="1.0" encoding="utf-8"?>
<RegexSnippets xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <RegexSnippet Version="1.0.0" Format="1.0.0">
        <Header>
            <Category>GroupingConstructs</Category>
            <Name>GroupOptions</Name>
            <Title>Group Options</Title>
            <Author>Pihrtsoft</Author>
            <Description>Applies or disables options.</Description>
            <SnippetKinds>
                <SnippetKind>Regular</SnippetKind>
                <SnippetKind>Surround</SnippetKind>
            </SnippetKinds>
            <Engines>
                <Engine>DotNet</Engine>
            </Engines>
        </Header>
        <Snippet>
            <Literals>
                <Literal>
                    <Id>apply</Id>
                    <Default>imnsx</Default>
                    <Description>Options to apply</Description>
                </Literal>
                <Literal>
                    <Id>disable</Id>
                    <Default>-imnsx</Default>
                    <Description>Options to disable (optional)</Description>
                </Literal>
            </Literals>
            <Codes>
                <Code><![CDATA[(?%apply%%disable%:%selected%%end%)]]></Code>
                <Code Kind="Format" Options="IgnorePatternWhitespace"><![CDATA[(?%apply%%disable%:
    %selected%%end%
)]]></Code>
            </Codes>
        </Snippet>
    </RegexSnippet>
</RegexSnippets>