<?xml version="1.0" encoding="utf-8"?>
<RegexSnippets xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <RegexSnippet Version="1.0.0" Format="1.0.0">
        <Header>
            <Category>Anchors</Category>
            <Name>Contiguous</Name>
            <Title>Contiguous Matches</Title>
            <Author>Pihrtsoft</Author>
            <Description>The match must occur at the position where the previous match ended.</Description>
            <SnippetKinds>
                <SnippetKind>Regular</SnippetKind>
            </SnippetKinds>
            <Engines>
                <Engine>DotNet</Engine>
            </Engines>
        </Header>
        <Snippet>
            <Codes>
                <Code><![CDATA[\G]]></Code>
            </Codes>
        </Snippet>
    </RegexSnippet>
</RegexSnippets>